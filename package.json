{
  "name": "digital-citizenship-portal",
  "version": "0.1.0",
  "main": "./build/app",
  "typings": "src/typings",
  "license": "MIT",
  "engines": {
    "node": "6.11.2",
    "npm": "6.1.0"
  },
  "scripts": {
    "start": "node build/app",
    "prepare": "patch-package",
    "build:tsc": "tsc --outDir build",
    "build": "npm-run-all -s generate:api-models build:tsc",
    "generate:api-models:public": "gen-api-models --api-spec https://raw.githubusercontent.com/teamdigitale/digital-citizenship-functions/master/api/public_api_v1.yaml --no-strict --out-dir src/api",
    "generate:api-models:admin": "gen-api-models --api-spec https://raw.githubusercontent.com/teamdigitale/digital-citizenship-functions/master/api/admin_api.yaml --no-strict --out-dir src/api",
    "generate:api-models": "npm-run-all -s generate:api-models:*"
  },
  "dependencies": {
    "@types/memoizee": "^0.4.2",
    "applicationinsights": "^1.0.3",
    "azure-arm-apimanagement": "^2.1.1-preview",
    "azure-graph": "^3.1.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "fp-ts": "^1.8.1",
    "helmet": "^3.9.0",
    "helmet-csp": "^2.6.0",
    "io-ts": "^1.3.0",
    "italia-ts-commons": "^2.14.0",
    "json-set-map": "^1.0.2",
    "memoizee": "^0.4.14",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "ms-rest": "^2.2.4",
    "ms-rest-azure": "^2.4.4",
    "node-fetch": "^2.2.0",
    "passport": "*",
    "passport-azure-ad": "^3.0.8",
    "patch-package": "^5.1.1",
    "randomstring": "^1.1.5",
    "referrer-policy": "^1.1.0",
    "request": "^2.83.0",
    "ulid": "^2.3.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.34",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/helmet": "^0.0.38",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.7.35",
    "@types/node-fetch": "^2.1.2",
    "@types/passport": "^0.4.5",
    "@types/randomstring": "^1.1.6",
    "@types/request": "^2.0.7",
    "@types/winston": "^2.3.7",
    "italia-utils": "^4.0.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.7.4",
    "ts-node": "^6.1.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-immutable": "^4.4.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.1"
  }
}
